      onDragLeave={handleDragLeave}\n            onDrop={handleDrop}\n            draggable=\"true\"\n            onClick={handleClick}\n        >\n            {index + 1}.\n            <a\n                id={'song-' + index + '-link'}\n                className=\"song-link\"\n                href={\"https://www.youtube.com/watch?v=\" + song.youTubeId}>\n                {song.title} by {song.artist}\n            </a>\n            <input\n                type=\"button\"\n                id={\"remove-song-\" + index}\n                className=\"list-card-button\"\n                value={\"\\u2715\"}\n                onClick={handleRemoveSong}\n            />\n        </div>\n    );\n}\n\nexport default SongCard;","/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/components/SplashScreen.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/components/RegisterScreen.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/transactions/UpdateSong_Transaction.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/common/jsTPS.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/transactions/CreateSong_Transaction.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/transactions/MoveSong_Transaction.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/transactions/RemoveSong_Transaction.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/store/store-request-api/index.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/auth/auth-request-api/index.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/components/Copyright.js",[],"/Users/zhangwenhu/Desktop/Playlister_HW4/client/src/components/MUIErrorModal.js",[],{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":27,"column":8,"nodeType":"136","endLine":27,"endColumn":10,"suggestions":"137"},{"ruleId":"138","severity":1,"message":"139","line":53,"column":9,"nodeType":"140","messageId":"141","en